{
  "compilerOptions": {
    "outDir": "dist",
    "rootDir": "src",
    "lib": [
      "ESNext"
    ],
    "target": "ESNext",
    "module": "Preserve",
    "moduleResolution": "Bundler",
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": false,
    "allowImportingTsExtensions": true,
    "declaration": true,
    "emitDeclarationOnly": true,
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "allowJs": true,
    "checkJs": false,
    "noEmitOnError": false,
    "moduleDetection": "force",
    "allowArbitraryExtensions": true,
    "baseUrl": ".",
    "paths": {
      "@alanos/core": [
        "../../core/src"
      ],
      "@alanos/core/*": [
        "../../core/src/*"
      ]
    }
  },
  "include": [
    "src/**/*.ts"
  ]
}